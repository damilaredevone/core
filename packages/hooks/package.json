{
  "name": "@damilaredev/hooks",
  "version": "0.0.0",
  "description": "reusable react hooks",
  "keywords": [
    "react",
    "hooks",
    "reactjs"
  ],
  "author": "damilare anjorin",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./index.js"
    },
    "./*": "./*"
  },
  "imports": {
    "#hooks": "./dist/index.esm.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts",
    "*.d.cts",
    "*.mjs",
    "*.js",
    "index.js"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c --watch",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@damilaredev/utils": "0.5.0"
  }
}
