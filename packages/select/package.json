{
    "name": "@damilaredev/select",
    "version": "0.0.0",
    "description": "select",
    "author": "damilare anjorin",
    "license": "MIT",
    "engines": {
      "node": "^14.16.0 || ^16.10.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
    },
    "publishConfig": {
      "access": "public"
    },
    "sideEffects": false,
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.esm.js",
        "require": "./index.js"
      },
      "./*": "./*"
    },
    "imports": {
      "#components": "./dist/index.esm.js"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.esm.js",
    "types": "./dist/index.d.ts",
    "files": [
      "dist",
      "*.d.ts",
      "*.d.cts",
      "*.mjs",
      "*.js",
      "index.js"
    ],
    "scripts": {
      "build": "rimraf dist && rollup -c",
      "dev": "rollup -c --watch",
      "prepublishOnly": "pnpm build"
    },
    "keywords": [
      "react",
      "components",
      "reactjs"
    ],
    "peerDependencies": {
        "@damilaredev/hooks": "*",
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "dependencies": {
      "@damilaredev/utils": "0.5.0"
    },
    "devDependencies": {
      "@damilaredev/hooks": "workspace:*"
    }
  }
